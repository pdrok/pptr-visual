	"dependencies": {
		"@percy/cli": "^1.0.0-beta.46",
		"@percy/puppeteer": "^2.0.0",
		"jest": "^26.6.3",
		"jest-image-snapshot": "^4.4.1",
		"prettier": "^2.2.1",
		"puppeteer": "^8.0.0"
	}
}
